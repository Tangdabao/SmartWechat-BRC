/** 标题字体样式  **/
.textTitle {
  position:relative;
  top:100px;
  left: 50px;
  font-size: 20px; /*设置文字字号*/
  color:black; /*设置文字颜色*/
  font-weight: bold; /*设置字体加粗*/
}

/* button按钮样式 */
.btn-area button{
